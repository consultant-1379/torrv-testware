                </methods>
            </class>
        </classes>
    </test>
    <test name="Create and List Statistical Subscriptions.">
        <parameter name="testId" value="TORRV-ZEPHYR-TC-635" />
        <parameter name="testTitle"
            value="Group07_PMIC_Inactive_Tests. Create and List Statistical Subscriptions." />
        <parameter name="pmic.create.ui.subscription" value="271_create_subscription.csv" />
        <classes>
            <class
                name="com.ericsson.nms.rv.taf.test.pmic.cases.CreateSubscription_AddNodes_SelectCountersAndSetScheduleUiTest" />
